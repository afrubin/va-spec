{
   "$schema": "https://json-schema.org/draft/2020-12/schema",
   "title": "ClinGen Categorical Variation Descriptor Schema",
   "type": "object",
   "description": "A message on the ClinGen clinvar-public stream for variation types",
   "allOf": [
      {
         "$ref": "https://raw.githubusercontent.com/ga4gh/vrsatile/clingen-variation-schema/schema/vod.json#/$defs/CategoricalVariationDescriptor"
      },
      {
         "$ref": "#/$defs/ClinGenCategoricalVariationDescriptor"
      }
   ],
   "$defs": {
      "ClinGenCategoricalVariationDescriptor": {
         "additionalProperties": true,
         "description": "Adds fields to CategoricalVariationDescriptor",
         "type": "object",
         "properties": {
            "is_version_of": {
               "description": "Identifier for the concept this represents, absent version.",
               "type": "string"
            }
         }
      }
   }
}